    <html lang="en">

    <%- include('./partials/head.ejs') %>
    <body>

      <%- include('./partials/nav.ejs') %>


      <div class="create-course">
        <form action="/courses" method="POST">
          <label for="title">Course Title: </label>
          <input type="text" id="title" name="title" required>
          <label for="instructor">Course Instructor: </label>
          <input type="text" id="instructor" name="instructor" required>
          <label for="description">Course Description: </label>
          <input type="text" id="description" name="description" required>
          <label for="schedule">Course Schedule/Meeting Time: </label>
          <input type="text" id="schedule" name="schedule" required>
          <label for="campus">Campus: </label>
          <input type="text" id="campus" name="campus" required>
          <label for="subject">Course Subject: </label>
          <input type="text" id="subject" name="subject" required>
          <label for="credits">Course Credits: </label>
          <input type="number" id="credits" name="credits" required>
          <button>Submit</button> 
        </form>
      </div>
      <%- include('./partials/footer.ejs') %>
    </body>
    </html>